- var whatPage = "contact";

<!DOCTYPE html>
html(lang="en")

  head

    include includes/head.pug

  body

    //- Preloader
    div(id="preloader")
        div(class="spinner")

    //- Navbar
    include sections/navbar.pug

    // Google Map
    section(class="map")
      iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1648.8281071127221!2d-6.584677927991222!3d34.257314996561796!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xda759e47ecddc9d%3A0xb91d5f61b44f6ac7!2sAvenue+Moulay+Abderrahmane%2C+K%C3%A9nitra%2C+Maroc!5e0!3m2!1sfr!2s!4v1494342981324" style="border:0;" allowfullscreen="")
    
    // Contact Form
    section(class="contact-page bg-light")
      div(class="container")
        div(class="row justify-content-center")
          div(class="col-lg-9")
            div(class="contact-wrap")
              div(class="section-title")
                h2 Contactez-nous
                span(class="section-title-divider")
              div(class="contact-form")
                form(action="#")
                  div(class="form-group")
                    label(for="name") Votre Nom 
                      span *
                    input(type="text" class="form-control" id="name")
                  div(class="form-group")
                    label(for="email") Votre E-Mail 
                      span *
                    input(type="email" class="form-control" id="email")
                  div(class="form-group")
                    label(for="subject") Sujet
                    input(type="text" class="form-control" id="subject")
                  div(class="form-group")
                    label(for="message") Votre Message 
                      span *
                    textarea(class="form-control" id="message" rows="3")
                  button(type="submit" class="btn btn-custom") Envoyer
    
    //- Vehicules Slider Section
    include sections/vehicles-slider.pug

    //- Footer
    include sections/footer.pug

    // Scripts
    include includes/scripts.pug